<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="gubi_purchase_order_generate_form">
        <field name="name">gubi.purchase.order.generate.form</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form" />
        <field name="arch" type="xml">
            <button name="button_cancel" position="before">
                <button
                    name="generate_purchase_order_button"
                    string="Generate UBL Order"
                    type="object"
                />
            </button>
        </field>
    </record>
</odoo>
